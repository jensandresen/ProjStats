@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="http://rygine.com/projects/looper.js/css/looper.css">

    <style type="text/css">
        html, body {
            height: 100%;
        }
         img {
             width: 100%;
             max-height: 80%;
         }
    </style>
</head>
<body>

<div data-looper="go" class="looper slide" data-interval="1000">
    <div class="looper-inner">
        <div class="item">
            <img src="https://docs.google.com/spreadsheets/d/1c1GUMtLWJ6hPVN0LzMDYp13aUNZmXCm_ZL_Qd5oT4PI/pubchart?oid=74348526&format=image"/>
        </div>
        <div class="item">
            <img src="https://docs.google.com/spreadsheets/d/1c1GUMtLWJ6hPVN0LzMDYp13aUNZmXCm_ZL_Qd5oT4PI/pubchart?oid=817085918&format=image"/>
        </div>
    </div>
</div>

<script src="http://rygine.com/projects/looper.js/docs/js/jquery-1.8.3.min.js"></script>
<script src="http://rygine.com/projects/looper.js/src/looper.js"></script>

</body>
</html>